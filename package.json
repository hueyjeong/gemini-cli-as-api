{
	"name": "gemini-cli-as-api",
	"version": "1.1.0",
	"description": "Gemini Native API for Google Gemini models via OAuth (Bun Runtime)",
	"private": true,
	"scripts": {
		"dev": "bun run src/node-server.ts",
		"start": "bun run src/node-server.ts",
		"format": "prettier --write src",
		"format:check": "prettier --check src",
		"lint": "eslint --ext .ts src",
		"lint:fix": "eslint --ext .ts src --fix",
		"docker:build": "docker-compose build",
		"docker:dev": "docker-compose up --build",
		"docker:start": "docker-compose up",
		"docker:stop": "docker-compose down",
		"docker:clean": "docker-compose down -v && docker system prune -f",
		"docker:logs": "docker-compose logs -f gemini-worker",
		"docker:shell": "docker-compose exec gemini-worker sh"
	},
	"dependencies": {
		"hono": "^4.9.2",
		"undici": "^6.20.1"
	},
	"devDependencies": {
		"@types/node": "^24.3.0",
		"@typescript-eslint/eslint-plugin": "^8.39.1",
		"@typescript-eslint/parser": "^8.50.0",
		"eslint": "^9.33.0",
		"prettier": "^3.6.2",
		"typescript": "^5.4.5"
	}
}
